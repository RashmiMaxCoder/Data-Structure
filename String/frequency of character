#include <bits/stdc++.h>
using namespace std;

int main() {
	string str="geeksforgeeks";
	int count[26]={0};
	for(int i=0;i<26;i++)
	{
		count[str[i]-'a']++;
	}
	for(int i=0;i<26;i++)
	{
		if(count[i]>0)
		cout<<char(i+'a')<<" "<<count[i]<<endl;
	}
	
	return 0;
}
//output:
-e 4
f 1
g 2
k 2
o 1
r 1
s 2

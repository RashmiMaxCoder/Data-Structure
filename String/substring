#include <bits/stdc++.h>
using namespace std;

int main() {
	
	string str="geeksforgeeks";
	cout<<str<<endl;
	cout<<str.length()<<endl;
	str=str+"xyz"; //concatination
	cout<<str<<endl;
	cout<<str.substr(1,3)<<endl; //begin and end index for subsrting
	cout<<str.substr(2,4)<<endl;
	cout<<str.find("eek")<<" "; // index of first occurancea
	return 0;
}
/*output-->>
geeksforgeeks
13
geeksforgeeksxyz
eek
eksf
1  */
